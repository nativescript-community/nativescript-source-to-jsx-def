{
  "name": "nativescript-source-to-jsx-def",
  "version": "1.0.0",
  "description": "Introspect the NativeScript source to generate JSX definitions for `svelte-type-checker`",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node dist/index.js",
